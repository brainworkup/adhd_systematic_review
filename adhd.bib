@article{izdebskaPersonalityDisordersAdult2021,
  title = {Personality {{Disorders}} in {{Adult Female Child Sexual Abuse Survivors}}: {{Dimensions}} of {{Personality Pathology}} and {{Characteristics}} of {{Abuse}}},
  shorttitle = {Personality {{Disorders}} in {{Adult Female Child Sexual Abuse Survivors}}},
  author = {Izdebska, Agnieszka},
  date = {2021-12},
  journaltitle = {J Interpers Violence},
  volume = {36},
