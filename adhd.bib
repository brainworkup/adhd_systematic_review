@article{bernabeiElectrocorticographyStereoEEG2021,
  title = {Electrocorticography and Stereo {{EEG}} Provide Distinct Measures of Brain Connectivity: Implications for Network Models},
  shorttitle = {Electrocorticography and Stereo {{EEG}} Provide Distinct Measures of Brain Connectivity},
  author = {Bernabei, John M and Arnold, T Campbell and Shah, Preya and Revell, Andrew and Ong, Ian Z and Kini, Lohith G and Stein, Joel M and Shinohara, Russell T and Lucas, Timothy H and Davis, Kathryn A and Bassett, Danielle S and Litt, Brian},
  date = {2021-07-01},
