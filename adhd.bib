  author = {Goodman, Marianne and Carpenter, David and Tang, Cheuk Y. and Goldstein, Kim E. and Avedon, Jennifer and Fernandez, Nicolas and Mascitelli, Kathryn A. and Blair, Nicholas J. and New, Antonia S. and Triebwasser, Joseph and Siever, Larry J. and Hazlett, Erin A.},
  date = {2014-10},
  journaltitle = {Journal of Psychiatric Research},
  volume = {57},
