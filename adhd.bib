  ids = {kahyaDifficultiesEmotionRegulation2023},
  title = {Difficulties in {{Emotion Regulation Mediated}} the {{Relationship Between Reflective Functioning}} and {{Borderline Personality Symptoms Among Non-Clinical Adolescents}}},
