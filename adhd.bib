@article{epsteinPsychosocialInterventionsChild2015,
  title = {Psychosocial {{Interventions}} for {{Child Disruptive Behaviors}}: {{A Meta-analysis}}},
  shorttitle = {Psychosocial {{Interventions}} for {{Child Disruptive Behaviors}}},
  author = {Epstein, Richard A. and Fonnesbeck, Christopher and Potter, Shannon and Rizzone, Katherine H. and McPheeters, Melissa},
  date = {2015-11-01},
  journaltitle = {Pediatrics},
  volume = {136},
