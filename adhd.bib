  author = {Farrow, Elizabeth and Chiocchetti, Andreas G. and Rogers, Jack C. and Pauli, Ruth and Raschle, Nora M. and Gonzalez-Madruga, Karen and Smaragdi, Areti and Martinelli, Anne and Kohls, Gregor and Stadler, Christina and Konrad, Kerstin and Fairchild, Graeme and Freitag, Christine M. and Chechlacz, Magdalena and De Brito, Stephane A.},
  date = {2021-09-24},
  journaltitle = {Transl Psychiatry},
