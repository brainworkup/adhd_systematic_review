@article{VanKempen2019,
  title = {Behavioural and Neural Signatures of Perceptual Decision-Making Are Modulated by Pupil-Linked Arousal},
  author = {family=Kempen, given=Jochem, prefix=van, useprefix=true and Loughnane, Gerard M. and Newman, Daniel P. and Kelly, Simon P. and Thiele, Alexander and O'Connell, Redmond G and Bellgrove, Mark A.},
