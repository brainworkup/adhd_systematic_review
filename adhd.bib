@article{vytalInducedanxietyDifferentiallyDisrupts2016,
  title = {Induced-Anxiety Differentially Disrupts Working Memory in Generalized Anxiety Disorder},
  author = {Vytal, Katherine E and Arkin, Nicole E and Overstreet, Cassie and Lieberman, Lynne and Grillon, Christian},
