@article{sanchez-cubilloConstructValidityTrail2009,
  title = {Construct Validity of the {{Trail Making Test}}: {{Role}} of Task-Switching, Working Memory, Inhibition/Interference Control, and Visuomotor Abilities},
  author = {Sánchez-Cubillo, I. and Periáñez, J. A. and Adrover-Roig, D. and Rodríguez-Sánchez, J. M. and Ríos-Lago, M. and Tirapu, J. and Barceló, F.},
