@article{waldmanExternalValidationBifactor2021,
  title = {External Validation of a Bifactor Model of Oppositional Defiant Disorder},
  author = {Waldman, Irwin D. and Rowe, Richard and Boylan, Khrista and Burke, Jeffrey D.},
  date = {2021-02},
  journaltitle = {Mol Psychiatry},
  volume = {26},
