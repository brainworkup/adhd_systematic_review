  rating = {5},
  keywords = {notion},
  file = {/Users/joey/Zotero/storage/NBK497XB/Guilmette et al. - 2020 - American Academy of Clinical Neuropsychology conse.pdf}
}

@article{gustavsonGeneticEnvironmentalInfluences2018,
  title = {Genetic and {{Environmental Influences}} on {{Verbal Fluency}} in {{Middle Age}}: {{A Longitudinal Twin Study}}},
  author = {Gustavson, Daniel E. and Panizzon, Matthew S. and Elman, Jeremy A. and Franz, Carol E. and Beck, Asad and Reynolds, Chandra A. and Jacobson, Kristen C. and Xian, Hong and Toomey, Rosemary and Lyons, Michael J. and Kremen, William S.},
