  author = {Meehan, Alan J. and Lewis, Stephanie J. and Fazel, Seena and Fusar-Poli, Paolo and Steyerberg, Ewout W. and Stahl, Daniel and Danese, Andrea},
  date = {2022-06},
  journaltitle = {Mol Psychiatry},
