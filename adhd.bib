@article{yangGCTAToolGenomewide2011,
  title = {{{GCTA}}: {{A Tool}} for {{Genome-wide Complex Trait Analysis}}},
  author = {Yang, Jian and Lee, Sang Hong and Goddard, Michael E and Visscher, Peter M},
  date = {2011-01},
  journaltitle = {Am. J. Hum. Genet.},
  volume = {88},
