@article{houlihanReplicationStudyCandidate2009,
  title = {Replication Study of Candidate Genes for Cognitive Abilities: {{The Lothian Birth Cohort}} 1936},
  author = {Houlihan, L M and Harris, S E and Luciano, M and Gow, A J and Starr, J M and Visscher, P M and Deary, I. J.},
  date = {2009},
  journaltitle = {Genes, Brain Behav.},
  volume = {8},
  number = {2},
