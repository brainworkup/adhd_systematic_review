@article{denoudenTransdiagnosticPhenotypesCompulsive2022,
  title = {Transdiagnostic Phenotypes of Compulsive Behavior and Associations with Psychological, Cognitive, and Neurobiological Affective Processing},
  author = {Den Ouden, Lauren and Suo, Chao and Albertella, Lucy and Greenwood, Lisa-Marie and Lee, Rico S. C. and Fontenelle, Leonardo F. and Parkes, Linden and Tiego, Jeggan and Chamberlain, Samuel R. and Richardson, Karyn and Segrave, Rebecca and Yücel, Murat},
