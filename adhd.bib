@article{knowlesGenomewideSignificantLocalization2014,
  title = {Genome-Wide Significant Localization for Working and Spatial Memory: {{Identifying}} Genes for Psychosis Using Models of Cognition},
  author = {Knowles, Emma E.M. and family=Carless, given=Melanie, prefix=a, useprefix=false and family=Almeida, given=Marcio A.A., prefix=de, useprefix=true and Curran, Joanne E and McKay, D Reese and Sprooten, Emma and Dyer, Thomas D and Göring, Harald H and Olvera, Rene and Fox, Peter and Almasy, Laura and Duggirala, Ravi and Kent, Jack W and Blangero, John and Glahn, David C},
  date = {2014-01},
  journaltitle = {Am. J. Med. Genet. Part B Neuropsychiatr. Genet.},
  volume = {165},
