  date = {2019-12},
  journaltitle = {Transl. Psychiatry},
  volume = {9},
  number = {1},
  pages = {77},
  publisher = {Springer US},
  issn = {2158-3188},
  doi = {10.1038/s41398-019-0402-0},
  url = {http://dx.doi.org/10.1038/s41398-019-0402-0 http://www.nature.com/articles/s41398-019-0402-0},
  abstract = {Developmental dyslexia (DD) is one of the most prevalent learning disorders, with high impact on school and psychosocial development and high comorbidity with conditions like attention-deficit hyperactivity disorder (ADHD), depression, and anxiety. DD is characterized by deficits in different cognitive skills, including word reading, spelling, rapid naming, and phonology. To investigate the genetic basis of DD, we conducted a genome-wide association study (GWAS) of these skills within one of the largest studies available, including nine cohorts of reading-impaired and typically developing children of European ancestry (N = 2562-3468). We observed a genome-wide significant effect (p \textbackslash textless 1 × 10-8) on rapid automatized naming of letters (RANlet) for variants on 18q12.2, within MIR924HG (micro-RNA 924 host gene; rs17663182 p = 4.73 × 10-9), and a suggestive association on 8q12.3 within NKAIN3 (encoding a cation transporter; rs16928927, p = 2.25 × 10-8). rs17663182 (18q12.2) also showed genome-wide significant multivariate associations with RAN measures (p = 1.15 × 10-8) and with all the cognitive traits tested (p = 3.07 × 10-8), suggesting (relational) pleiotropic effects of this variant. A polygenic risk score (PRS) analysis revealed significant genetic overlaps of some of the DD-related traits with educational attainment (EDUyears) and ADHD. Reading and spelling abilities were positively associated with EDUyears (p ∼ [10-5-10-7]) and negatively associated with ADHD PRS (p ∼ [10-8-10-17]). This corroborates a long-standing hypothesis on the partly shared genetic etiology of DD and ADHD, at the genome-wide level. Our findings suggest new candidate DD susceptibility genes and provide new insights into the genetics of dyslexia and its comorbities.},
  file = {/Users/joey/Zotero/storage/KBFFCUML/Gialluisi et al. - 2019 - Genome-wide association scan identifies new variants associated with a cognitive predictor of dyslexia.pdf}
}

@article{gloyDecisionMakingUncertainty2020,
