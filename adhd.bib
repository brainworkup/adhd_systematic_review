@article{chengMRLDPTwosampleMendelian2019,
  title = {{{MR-LDP}}: A Two-Sample {{Mendelian}} Randomization for {{GWAS}} Summary Statistics Accounting for Linkage Disequilibrium and Horizontal Pleiotropy},
  author = {Cheng, Qing and Yang, Yi and Shi, Xingjie and Yeung, Kar-Fu and Yang, Can and Peng, Heng and Liu, Jin},
