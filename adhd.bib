  shorttitle = {Pharmacological and Psychosocial Treatments for Adolescents with {{ADHD}}},
  author = {Sibley, Margaret H. and Kuriyan, Aparajita B. and Evans, Steven W. and Waxmonsky, James G. and Smith, Bradley H.},
