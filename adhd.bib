@article{simonCognitiveFunctioningSchizophrenia2007,
  title = {Cognitive Functioning in the Schizophrenia Prodrome.},
  author = {Simon, Andor E and Cattapan-Ludewig, Katja and Zmilacher, Solange and Arbach, Dima and Gruber, Kerstin and Dvorsky, Diane N and Roth, Binia and Isler, Emanuel and Zimmer, Alexander and Umbricht, Daniel},
  date = {2007-03},
  journaltitle = {Schizophr. Bull.},
  volume = {33},
  number = {3},
  eprint = {17412711},
  eprinttype = {pmid},
  pages = {761--771},
  issn = {0586-7614},
  doi = {10.1093/schbul/sbm018},
  url = {https://academic.oup.com/schizophreniabulletin/article-lookup/doi/10.1093/schbul/sbm018},
  abstract = {In the last decade, there has been an increasing interest in cognitive alterations during the early course of schizophrenia. From a clinical perspective, a better understanding of cognitive functioning in putative at-risk states for schizophrenia is essential for developing optimal early intervention models. Two approaches have more recently been combined to assess the entire course of the initial schizophrenia prodrome: the predictive "basic symptom at-risk" (BS) and the ultra high-risk (UHR) criteria. Basic symptoms are considered to be present during the entire disease progression, including the initial prodrome, while the onset of symptoms captured by the UHR criteria expresses further disease progression toward frank psychosis. The present study investigated the cognitive functioning in 93 subjects who met either BS or UHR criteria and thus were assumed to be at different points on the putative trajectory to psychosis. We compared them with 43 patients with a first episode of psychosis and to 49 help-seeking patient controls. All groups performed significantly below normative values. Both at-risk groups performed at intermediate levels between the first-episode (FE) group and normative values. The UHR group demonstrated intermediate performance between the FE and BS groups. Overall, auditory working memory, verbal fluency/processing speed, and declarative verbal memory were impaired the most. Our results suggest that cognitive impairments may still be modest in the early stages of the initial schizophrenia prodrome and thus support current efforts to intervene in the early course of impending schizophrenia because early intervention may prevent or delay the onset of frank psychosis and thus prevent further cognitive damage.},
  langid = {english},
  keywords = {Adolescent,Adult,Attention,Cognition Disorders,diagnosis,Disease Progression,Female,Humans,Intelligence,Male,Memory,Neuropsychological Tests,psychology,Reaction Time,Risk,Schizophrenia,Schizophrenic Language,Schizophrenic Psychology,Schizotypal Personality Disorder,Short-Term,Switzerland,Verbal Behavior,Verbal Learning}
}

@article{sinclairPossibleAssociationAPOE2015,
  title = {Possible Association of {{APOE}} Genotype with Working Memory in Young Adults},
  author = {Sinclair, Lindsey I. and Button, Katherine S. and Munafò, Marcus R. and Day, Ian N.M. and Lewis, Glyn},
  date = {2015},
  journaltitle = {PLoS One},
  volume = {10},
