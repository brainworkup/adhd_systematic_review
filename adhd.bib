@article{reubenAssociationChildhoodBlood2017,
  title = {Association of Childhood Blood Lead Levels with Cognitive Function and Socioeconomic Status at Age 38 Years and with {{IQ}} Change and Socioeconomic Mobility between Childhood and Adulthood},
  author = {Reuben, Aaron and Caspi, Avshalom and Belsky, Daniel W and Broadbent, Jonathan and Harrington, Honalee and Sugden, Karen and Houts, Renate M and Ramrakha, Sandhya and Poulton, Richie and Moffitt, Terrie E},
